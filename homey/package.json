{
  "name": "homey",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "compile:sass": "node-sass sass/main.scss public/css/style.css -w",
    "prod_compile:sass": "node-sass ./sass/main.scss ./public/css/style.css --output-style compressed",
    "dev": "concurrently --kill-others \"npm run compile:sass\" \"npm run serve\""
  },
  "dependencies": {
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/compiler-sfc": "^3.2.28",
    "@kyvg/vue3-notification": "^2.3.4",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^8.3.0",
    "babel-eslint": "^10.1.0",
    "@material-design-icons/font": "^0.10.6",
    "@tweenjs/tween.js": "^18.6.4",
    "axios": "^0.25.0",
    "chart.js": "^3.7.0",
    "core-js": "^3.20.3",
    "js-yaml": "^4.1.0",
    "node-sass": "^7.0.1",
    "raw-loader": "^4.0.2",
    "three": "^0.136.0",
    "vue": "^3.0.0",
    "vue-axios": "^3.4.0",
    "vue-chart-3": "^3.0.9",
    "vue-skycons": "^4.1.1"
  },
  "devDependencies": {
    "@types/three": "^0.136.1",
    "concurrently": "^7.0.0"    
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
