{
  "name": "homey",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "compile:sass": "sass ./sass/main.scss public/css/style.css -w",
    "prod_compile:sass": "sass ./sass/main.scss ./public/css/style.css --style compressed",
    "dev": "concurrently --kill-others \"npm run compile:sass\" \"npm run serve\""
  },
  "dependencies": {
    "@kyvg/vue3-notification": "^2.3.4",
    "@tweenjs/tween.js": "^18.6.4",
    "axios": "^0.25.0",
    "chart.js": "^3.7.0",
    "core-js": "^3.8.3",
    "js-yaml": "^4.1.0",
    "raw-loader": "^4.0.2",
    "three": "^0.137.5",
    "vue": "^3.2.13",
    "vue-axios": "^3.4.0",
    "vue-chart-3": "^3.0.9",
    "vue-skycons": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@types/three": "^0.137.0",
    "@vue/cli-plugin-babel": "~5.0.0-rc.2",
    "@vue/cli-plugin-eslint": "~5.0.0-rc.2",
    "@vue/cli-service": "~5.0.0-rc.2",
    "@yushijinhun/three-minifier-webpack": "^0.3.1",
    "compression-webpack-plugin": "^9.2.0",
    "concurrently": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "html-minimizer-webpack-plugin": "^3.5.0",
    "sass": "^1.49.7",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
