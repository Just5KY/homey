{
  "name": "homey",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "compile:sass": "node-sass sass/main.scss public/css/style.css -w",
    "prod_compile:sass": "node-sass sass/main.scss public/css/style.css",
    "dev": "concurrently --kill-others \"npm run compile:sass\" \"npm run serve\""
  },
  "dependencies": {
    "@kyvg/vue3-notification": "^2.3.4",
    "axios": "^0.24.0",
    "core-js": "^3.6.5",
    "vue": "^3.0.0",
    "vue-axios": "^3.4.0",
    "vue-chart-3": "^3.0.4",
    "vue-skycons": "^4.1.1"
  },
  "devDependencies": {
    "@material-design-icons/font": "^0.10.4",
    "@tweenjs/tween.js": "^18.6.4",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.4.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "js-yaml": "^3.14.1",
    "node-sass": "^6.0.1",
    "raw-loader": "^4.0.2",
    "three": "^0.135.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
